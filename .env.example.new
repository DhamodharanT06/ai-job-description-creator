# Copy this file to `.env` for local development (do NOT commit your real keys).

# OpenRouter API key (required for AI generation)
OPENROUTER_API_KEY=

# Optional: model to use (defaults to meta-llama/llama-3.3-70b-instruct:free if unset)
OPENROUTER_MODEL=

# Optional: override base API URL
OPENROUTER_BASE_URL=

# Optional: VERCEL_URL set by Vercel in production; useful for diagnostics
VERCEL_URL=
